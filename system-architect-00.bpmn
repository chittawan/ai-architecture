<bpmn:process id="Order_Process">
  <bpmn:startEvent id="Start" name="User Order"/>
  <bpmn:task id="OrderService" name="Process Order"/>
  <bpmn:task id="PaymentService" name="Handle Payment"/>
  <bpmn:task id="NotificationService" name="Send Notification"/>
  <bpmn:endEvent id="End" name="Order Completed"/>

  <bpmn:sequenceFlow sourceRef="Start" targetRef="OrderService"/>
  <bpmn:sequenceFlow sourceRef="OrderService" targetRef="PaymentService"/>
  <bpmn:sequenceFlow sourceRef="PaymentService" targetRef="NotificationService"/>
  <bpmn:sequenceFlow sourceRef="NotificationService" targetRef="End"/>
</bpmn:process>
