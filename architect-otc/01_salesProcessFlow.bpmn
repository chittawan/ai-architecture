<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" 
                  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" 
                  xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" 
                  xmlns:di="http://www.omg.org/spec/DD/20100524/DI" 
                  id="Definitions_1" targetNamespace="http://bpmn.io/schema/bpmn">
  
  <bpmn:collaboration id="Collaboration_1">
    <bpmn:participant id="Participant_1" name="Sales Process Flow" processRef="Process_1" />
  </bpmn:collaboration>
  
  <bpmn:process id="Process_1" isExecutable="true">
    
    <bpmn:laneSet id="LaneSet_1">
      <bpmn:lane id="Lane_SALE" name="Sales">
        <bpmn:flowNodeRef>act1</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>act8</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_KA" name="Key Account Management">
        <bpmn:flowNodeRef>act2</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>act7</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_RT" name="Retail">
        <bpmn:flowNodeRef>act3</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_OMS" name="Order Management System">
        <bpmn:flowNodeRef>act4</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_PP" name="Privilege &amp; Promotion">
        <bpmn:flowNodeRef>act5</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_PM" name="Payment">
        <bpmn:flowNodeRef>act6</bpmn:flowNodeRef>
      </bpmn:lane>
    </bpmn:laneSet>
    
    <!-- Start Event -->
    <bpmn:startEvent id="act1" name="เบิกของขึ้นรถ">
      <bpmn:outgoing>Flow_1_2</bpmn:outgoing>
    </bpmn:startEvent>
    
    <!-- Tasks -->
    <bpmn:task id="act2" name="check-in">
      <bpmn:incoming>Flow_1_2</bpmn:incoming>
      <bpmn:outgoing>Flow_2_3</bpmn:outgoing>
    </bpmn:task>
    
    <bpmn:task id="act3" name="ขายของหน้าร้าน">
      <bpmn:incoming>Flow_2_3</bpmn:incoming>
      <bpmn:outgoing>Flow_3_4</bpmn:outgoing>
    </bpmn:task>
    
    <bpmn:task id="act4" name="รับคำสั่งซื้อ">
      <bpmn:incoming>Flow_3_4</bpmn:incoming>
      <bpmn:incoming>Flow_5_4</bpmn:incoming>
      <bpmn:outgoing>Flow_4_5</bpmn:outgoing>
      <bpmn:outgoing>Flow_4_6</bpmn:outgoing>
    </bpmn:task>
    
    <bpmn:task id="act5" name="ตรวจสอบสิทธิ์ส่วนลด/โปรโมชั่น">
      <bpmn:incoming>Flow_4_5</bpmn:incoming>
      <bpmn:outgoing>Flow_5_4</bpmn:outgoing>
    </bpmn:task>
    
    <bpmn:task id="act6" name="รับชำระเงิน">
      <bpmn:incoming>Flow_4_6</bpmn:incoming>
      <bpmn:outgoing>Flow_6_7</bpmn:outgoing>
    </bpmn:task>
    
    <bpmn:task id="act7" name="check-out">
      <bpmn:incoming>Flow_6_7</bpmn:incoming>
      <bpmn:outgoing>Flow_7_8</bpmn:outgoing>
    </bpmn:task>
    
    <!-- End Event -->
    <bpmn:endEvent id="act8" name="เคลียร์เงิน">
      <bpmn:incoming>Flow_7_8</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- Sequence Flows -->
    <bpmn:sequenceFlow id="Flow_1_2" sourceRef="act1" targetRef="act2" />
    <bpmn:sequenceFlow id="Flow_2_3" sourceRef="act2" targetRef="act3" />
    <bpmn:sequenceFlow id="Flow_3_4" sourceRef="act3" targetRef="act4" />
    <bpmn:sequenceFlow id="Flow_4_5" sourceRef="act4" targetRef="act5" />
    <bpmn:sequenceFlow id="Flow_5_4" sourceRef="act5" targetRef="act4" />
    <bpmn:sequenceFlow id="Flow_4_6" sourceRef="act4" targetRef="act6" />
    <bpmn:sequenceFlow id="Flow_6_7" sourceRef="act6" targetRef="act7" />
    <bpmn:sequenceFlow id="Flow_7_8" sourceRef="act7" targetRef="act8" />
    
  </bpmn:process>
  
  <bpmndi:BPMNDiagram id="BPMNDiagram_1">
    <bpmndi:BPMNPlane id="BPMNPlane_1" bpmnElement="Collaboration_1">
      
      <bpmndi:BPMNShape id="Participant_1_di" bpmnElement="Participant_1" isHorizontal="true">
        <dc:Bounds x="120" y="80" width="1200" height="720" />
      </bpmndi:BPMNShape>
      
      <!-- Lane Shapes -->
      <bpmndi:BPMNShape id="Lane_SALE_di" bpmnElement="Lane_SALE" isHorizontal="true">
        <dc:Bounds x="150" y="80" width="1170" height="120" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Lane_KA_di" bpmnElement="Lane_KA" isHorizontal="true">
        <dc:Bounds x="150" y="200" width="1170" height="120" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Lane_RT_di" bpmnElement="Lane_RT" isHorizontal="true">
        <dc:Bounds x="150" y="320" width="1170" height="120" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Lane_OMS_di" bpmnElement="Lane_OMS" isHorizontal="true">
        <dc:Bounds x="150" y="440" width="1170" height="120" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Lane_PP_di" bpmnElement="Lane_PP" isHorizontal="true">
        <dc:Bounds x="150" y="560" width="1170" height="120" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Lane_PM_di" bpmnElement="Lane_PM" isHorizontal="true">
        <dc:Bounds x="150" y="680" width="1170" height="120" />
      </bpmndi:BPMNShape>
      
      <!-- Activity Shapes -->
      <bpmndi:BPMNShape id="act1_di" bpmnElement="act1">
        <dc:Bounds x="200" y="120" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="180" y="163" width="76" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="act2_di" bpmnElement="act2">
        <dc:Bounds x="300" y="240" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="act3_di" bpmnElement="act3">
        <dc:Bounds x="450" y="360" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="act4_di" bpmnElement="act4">
        <dc:Bounds x="600" y="480" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="act5_di" bpmnElement="act5">
        <dc:Bounds x="750" y="600" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="act6_di" bpmnElement="act6">
        <dc:Bounds x="900" y="720" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="act7_di" bpmnElement="act7">
        <dc:Bounds x="1050" y="240" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="act8_di" bpmnElement="act8">
        <dc:Bounds x="1200" y="120" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="1190" y="163" width="56" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <!-- Sequence Flow Edges -->
      <bpmndi:BPMNEdge id="Flow_1_2_di" bpmnElement="Flow_1_2">
        <di:waypoint x="236" y="138" />
        <di:waypoint x="268" y="138" />
        <di:waypoint x="268" y="280" />
        <di:waypoint x="300" y="280" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_2_3_di" bpmnElement="Flow_2_3">
        <di:waypoint x="400" y="280" />
        <di:waypoint x="425" y="280" />
        <di:waypoint x="425" y="400" />
        <di:waypoint x="450" y="400" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_3_4_di" bpmnElement="Flow_3_4">
        <di:waypoint x="550" y="400" />
        <di:waypoint x="575" y="400" />
        <di:waypoint x="575" y="520" />
        <di:waypoint x="600" y="520" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_4_5_di" bpmnElement="Flow_4_5">
        <di:waypoint x="700" y="520" />
        <di:waypoint x="725" y="520" />
        <di:waypoint x="725" y="640" />
        <di:waypoint x="750" y="640" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_5_4_di" bpmnElement="Flow_5_4">
        <di:waypoint x="750" y="640" />
        <di:waypoint x="675" y="640" />
        <di:waypoint x="675" y="560" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_4_6_di" bpmnElement="Flow_4_6">
        <di:waypoint x="700" y="520" />
        <di:waypoint x="800" y="520" />
        <di:waypoint x="800" y="760" />
        <di:waypoint x="900" y="760" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_6_7_di" bpmnElement="Flow_6_7">
        <di:waypoint x="1000" y="760" />
        <di:waypoint x="1025" y="760" />
        <di:waypoint x="1025" y="280" />
        <di:waypoint x="1050" y="280" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_7_8_di" bpmnElement="Flow_7_8">
        <di:waypoint x="1150" y="280" />
        <di:waypoint x="1175" y="280" />
        <di:waypoint x="1175" y="138" />
        <di:waypoint x="1200" y="138" />
      </bpmndi:BPMNEdge>
      
    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>
  
</bpmn:definitions>
